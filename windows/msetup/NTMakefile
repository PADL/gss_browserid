#
# Moonshot Setup
#

RELDIR=msetup

!include ../NTMakefile.w32

cdefines = $(cdefines) -DUNICODE -D_UNICODE

#
# msetup library
#
LIBMSETUP =$(LIBDIR)\libmsetup.lib

libmsetup_OBJS =    					\
	$(OBJ)\msetup_aaa.obj				\
	$(OBJ)\msetup_cred.obj				\
	$(OBJ)\msetup_flags.obj				\
	$(OBJ)\msetup_map.obj				\
	$(OBJ)\msetup_reg.obj				

$(LIBMSETUP): $(libmsetup_OBJS)
	$(LIBCON)

#
# msetup console utility
#
MSETUP = $(BINDIR)\msetup.exe

msetup_OBJS = $(OBJ)\msetup_cmd.obj

$(MSETUP): $(msetup_OBJS) $(LIBMSETUP)
	$(EXECONLINK) crypt32.lib

all:: $(LIBMSETUP) $(MSETUP)

clean::

